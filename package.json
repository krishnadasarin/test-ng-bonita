{
  "name": "zgwnu-ng-bonita-test",
  "version": "1.0.0",
  "description": "Angular Bonita Rest Api Services Testing Example by ZGW NU",
  "scripts": {
    "cfg:dev": "cp cfg/dev/*.* dest",
    "build:dev": "tsc -p src/",
    "cpx:dev": "cpx \"src/**/*.{html,css,json}\" dest",
    "cpx:watch:dev": "cpx \"src/**/*.{html,css,json}\" dest --watch",
    "build:watch:dev": "tsc -w -p src/",
    "serve:dev": "lite-server -c=bs-config.json",
    "prestart": "npm run build:dev && npm run cfg:dev && npm run cpx:dev",
    "start": "concurrently \"npm run build:watch:dev\" \"npm run serve:dev\" \"npm run cpx:watch:dev\" "
  },
  "author": "Onno Haldar",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.4.3",
    "@angular/compiler": "^4.4.3",
    "@angular/compiler-cli": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@angular/platform-browser": "^4.4.3",
    "@angular/platform-browser-dynamic": "^4.4.3",
    "core-js": "^2.5.1",
    "rxjs": "5.4.3",
    "systemjs": "0.20.19",
    "zone.js": "^0.8.18",
    "@zgwnu/ng-bonita": "1.0.4-alpha-2"
  },
  "devDependencies": {
    "@types/node": "^8.0.31",
    "@zgwnu/bonitacli": "git+https://github.com/onnohaldar/zgwnu-bonita-cli.git",
    "concurrently": "^3.5.0",
    "cpx": "^1.5.0",
    "lite-server": "^2.3.0",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "tslib": "^1.7.1",
    "typescript": "^2.5.3",
    "uglify-js": "^3.1.2"
  }
}
